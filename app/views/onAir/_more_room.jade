.container
	.row(ng-if='items.length > 8 ' style='margin: 30px 0;')
		.col-md-2(style='padding:5px; margin: 20px 0;' ng-repeat='ite in items' ng-if='$index > 8 && ite.onAir == true')
			.on-air-block-more
				img.pointer.img-responsive.room-more-image(
				src='{{ite.banner || ite.background || ite.User.avatar}}'
				ui-sref='base.room-detail({id : ite.id})'
				)
				.bottom
					.room-title {{ ite.title || ite.User.name }}
					.icons
						i.fa.fa-eye
						span.value.margin-left-5 {{ite.views | thousandSuffix : 1}}
						i.fa.fa-heart.margin-left-20
						span.value.margin-left-5 {{ite.hearts | thousandSuffix : 1}}

					button.btn.add-plus(ng-if='ite.isFollow == false'
					ng-click='onairClickFollowIdol(ite, $index, "follow")')
						i.fa.fa-plus
					button.btn.add-plus.unfollow(ng-if='ite.isFollow == true'
					ng-click='onairClickFollowIdol(ite, $index, "unfollow")')
						i.fa.fa-minus
				.on-air-icon
					.icon &nbsp;
					span LIVE
				//img.on-air-icon-play(src='/images/on_air_play.png' ui-sref='base.room-detail({id : ite.id})')